{
  "name": "helphive",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently --names \"FRONTEND,BACKEND\" --prefix-colors \"cyan,yellow\" \"npm run frontend\" \"npm run backend\"",
    "frontend": "vite --host 0.0.0.0 --port 3003",
    "backend": "cd server && npm run dev",
    "build": "vite build",
    "preview": "vite preview",
    "server": "nodemon -r dotenv/config server/index.js",
    "tw:build": "tailwindcss -i ./src/index.css -o ./src/output.css --watch",
    "start": "vite",
    "test": "vitest"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.12",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "jwt-decode": "^4.0.0",
    "mongoose": "^8.17.1",
    "nodemon": "^3.1.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.8.1",
    "react-toastify": "^11.0.5",
    "sweetalert2": "^11.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "tailwindcss": "^4.1.12",
    "vite": "^7.1.2"
  }
}
